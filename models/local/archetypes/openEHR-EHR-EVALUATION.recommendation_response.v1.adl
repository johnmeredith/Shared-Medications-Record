	>
	lifecycle_state = <"AuthorDraft">
	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To record details of a foral response to a clinical recommendation.">
		>
	>
	other_details = <
		["MD5-CAM-1.0.1"] = <"EDED4A8FD7DB851144A9E0DB37E4E56E">
